this.odooEditor.automaticStepSkipStack();const editorWindow=this.odooEditor.document.defaultView;const $target=e?editorWindow.$(e.target):editorWindow.$();this.toolbarEl.querySelector('#mediaParagraphDropdownButton')?.setAttribute('id','paragraphDropdownButton');const isInMedia=$target.is(mediaSelector)&&!$target.parent().hasClass('o_stars')&&e.target&&(e.target.isContentEditable||(e.target.parentElement&&e.target.parentElement.isContentEditable));this.toolbarEl.classList.toggle('oe-media',isInMedia);for(const el of this.toolbarEl.querySelectorAll(['#image-preview','#image-shape','#image-width','#image-padding','#image-edit','#media-replace',].join(','))){el.classList.toggle('d-none',!isInMedia);}
