for(const el of this.toolbarEl.querySelectorAll(['#image-transform','#image-crop','#media-description',].join(','))){el.classList.toggle('d-none',!isInMedia||!$target.is('img'));}
this.lastMediaClicked=isInMedia&&e.target;this.lastElement=$target[0];for(const el of this.toolbarEl.querySelectorAll(['#style','#decoration','#font-size','#justifyFull','#list','#colorInputButtonGroup','#media-insert','#chatgpt',].join(','))){el.classList.toggle('d-none',isInMedia);}
