odoo.define('@web_editor/js/wysiwyg/linkDialogCommand',['@web/core/registry','@web/core/commands/default_providers','@web_editor/js/wysiwyg/wysiwyg'],function(require){'use strict';let __exports={};const{registry}=require('@web/core/registry')
