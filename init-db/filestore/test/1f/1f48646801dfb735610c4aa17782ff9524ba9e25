let focusNode;let anchorOffset=0;let focusOffset;if(selection&&link.parentElement){if(shouldUnlink(this.linkToolsInfos.link,this.linkToolsInfos.colorCombinationClass)){if(link.childNodes.length){anchorNode=link.childNodes[0];focusNode=link.childNodes[link.childNodes.length-1];}else{const parent=link.parentElement;const index=Array.from(parent.childNodes).indexOf(link);anchorNode=focusNode=parent;anchorOffset=focusOffset=index;}}else{const commonBlock=selection.rangeCount&&closestBlock(selection.getRangeAt(0).commonAncestorContainer);[anchorNode,focusNode]=commonBlock&&link.contains(commonBlock)?[commonBlock,commonBlock]:[link,link];}
